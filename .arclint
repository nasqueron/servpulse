{
  "linters": {
    "chmod": {
      "type": "chmod"
    },
    "filename": {
      "type": "filename"
    },
    "json": {
      "type": "jsonlint",
      "include": [
        "(^\\.arcconfig$)",
        "(^\\.arclint$)",
        "(\\.json$)"
      ]
    },
    "merge-conflict": {
      "type": "merge-conflict"
    },
    "spelling": {
      "type": "spelling"
    }
  }
}
